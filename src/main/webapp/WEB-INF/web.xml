<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RecordController</servlet-name>
        <servlet-class>Controllers.RecordController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RadiologyController</servlet-name>
        <servlet-class>Controllers.RadiologyController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientController</servlet-name>
        <servlet-class>Controllers.PatientController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TreatmentController</servlet-name>
        <servlet-class>Controllers.TreatmentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterController</servlet-name>
        <servlet-class>Controllers.FilterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetPatientListController</servlet-name>
        <servlet-class>Controllers.GetPatientListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyImmunizationController</servlet-name>
        <servlet-class>Controllers.ModifyImmunizationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetPatientListController2</servlet-name>
        <servlet-class>Controllers.GetPatientListController2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAllergyController</servlet-name>
        <servlet-class>Controllers.DeleteAllergyController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RecordController</servlet-name>
        <url-pattern>/RecordController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RadiologyController</servlet-name>
        <url-pattern>/RadiologyController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientController</servlet-name>
        <url-pattern>/PatientController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TreatmentController</servlet-name>
        <url-pattern>/TreatmentController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterController</servlet-name>
        <url-pattern>/FilterController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetPatientListController</servlet-name>
        <url-pattern>/GetPatientListController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyImmunizationController</servlet-name>
        <url-pattern>/ModifyImmunizationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetPatientListController2</servlet-name>
        <url-pattern>/GetPatientListController2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAllergyController</servlet-name>
        <url-pattern>/DeleteAllergyController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
